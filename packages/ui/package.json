{
  "name": "@instabot.js/ui",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint **/*.js"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-react": "^7.7.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "react/jsx-filename-extension": [0],
      "react/prop-types": [2, {
        "ignore": ["children"]
      }]
    },
    "overrides": [
      {
        "files": ["**/*.test.js"],
        "env": {
          "jest": true
        }
      }
    ]
  }
}
