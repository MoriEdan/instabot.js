{
  "name": "instabot.js",
  "description": "An attempt to create instabot as gui app",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "start": "electron main.js",
    "start:prod": "NODE_ENV=production electron main.js",
    "build": "NODE_ENV=production poi build --generate-stats dist/stats.json",
    "test": "jest --colors",
    "lint": "eslint src/**/*.js"
  },
  "engines": {
    "node": "8.9.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "electron": "^2.0.0-beta.7",
    "electron-devtools-installer": "^2.2.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "given2": "^2.1.5",
    "jest": "^22.4.3",
    "mock-fs": "^4.4.2",
    "node-sass": "^4.8.3",
    "poi": "^9.6.13",
    "poi-preset-babel-minify": "^1.0.3",
    "sass-loader": "^6.0.7"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "deepmerge": "^2.1.0",
    "fs-extra": "^5.0.0",
    "ionicons": "^3.0.0",
    "lodash": "^4.17.5",
    "mobx": "^4.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "request-promise-native": "^1.0.5",
    "tough-cookie": "^2.3.4"
  },
  "externals": [
    "lodash",
    "fs-extra",
    "request-promise-native",
    "tough-cookie"
  ],
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "given": true,
      "moment": true
    },
    "rules": {
      "no-debugger": [
        0
      ],
      "global-require": [
        0
      ],
      "react/prop-types": [
        2,
        {
          "ignore": [
            "children"
          ]
        }
      ],
      "react/jsx-filename-extension": [
        0
      ],
      "react/prefer-stateless-function": [
        0
      ],
      "jsx-a11y/click-events-have-key-events": [
        0
      ],
      "jsx-a11y/no-static-element-interactions": [
        0
      ],
      "jsx-a11y/alt-text": [
        0
      ]
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "support/webpack.config.js"
        }
      }
    },
    "overrides": [
      {
        "files": [
          "**/*.spec.js"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./support/jest"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "useBuiltIns": true,
          "targets": {
            "node": "8.9.3"
          }
        }
      ]
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-class-properties",
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  }
}
